{
  "headers": [
    {
      "source": "/models/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "model/gltf-binary"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/models/(.*)",
      "destination": "/models/$1"
    }
  ]
} 